{
  "name": "vstack-graph",
  "version": "0.4.0",
  "description": "A RxJS object relation graph for client side JavaScript Applications",
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "postinstall": "npm run typings",
    "typings": "typings install",
    "start": "npm run test",
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-spec.json && npm run minify-js",
    "pretest": "npm run build",
    "test": "karma start karma.conf.js",
    "posttest": "",
    "coverage": "remap-istanbul -i coverage/coverage-final.json -o coverage -t html && http-server -c-1 -o -p 9875 ./coverage",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "minify-js": "uglifyjs ./bundles/vstack-graph.js -o ./bundles/vstack-graph.min.js",
    "clean": "rm -rf dist && rm -rf bundles"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vintage-software/vstack-graph.git"
  },
  "dependencies": {
    "rxjs": "5.0.0-beta.6"
  },
  "devDependencies": {
    "browserify-istanbul": "^2.0.0",
    "codecov.io": "^0.1.6",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.1",
    "karma-browserify": "^5.0.5",
    "karma-coverage": "^1.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "remap-istanbul": "^0.6.4",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "uglify-js": "^2.7.0",
    "watchify": "^3.7.0"
  },
  "keywords": [
    "angular2",
    "rxjs",
    "jasmine",
    "typescript",
    "rest",
    "services",
    "falcor"
  ],
  "bugs": {
    "url": "https://github.com/vintage-software/vstack-graph/issues"
  },
  "homepage": "https://github.com/vintage-software/vstack-graph"
}
