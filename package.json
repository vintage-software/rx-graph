{
  "name": "vstack-graph",
  "version": "0.1.3",
  "description": "A HTTP REST library for Angular 2 including relational data structure support.",
  "typings": "./dist/vstack-graph.d.ts",
  "main": "./bundles/vstack-graph.js",
  "scripts": {
    "postinstall": "npm run typings",
    "typings": "typings install",
    "start": "npm run test",
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-spec.json && npm run minify-js",
    "pretest": "npm run build",
    "test": "karma start karma.conf.js",
    "posttest": "",
    "coverage": "remap-istanbul -i coverage/coverage-final.json -o coverage -t html && http-server -c-1 -o -p 9875 ./coverage",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "minify-js": "uglifyjs ./bundles/vstack-graph.js -o ./bundles/vstack-graph.min.js",
    "clean": "rm -rf dist && rm -rf bundles"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vintage-software/vstack-graph.git"
  },
  "dependencies": {
    "rxjs": "5.0.0-beta.2"
  },
  "devDependencies": {
    "angular2": "2.0.0-beta.16",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "zone.js": "0.6.12",   
    "http-server": "0.8.5",
    "jasmine-core": "2.4.1",
    "karma": "0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-coverage": "0.5.3",
    "karma-jasmine": "0.3.6",
    "remap-istanbul": "0.5.1",
    "systemjs": "0.19.17",
    "typescript": "^1.7.5",
    "typings": "^0.6.8",
    "codecov.io": "0.1.6",
    "uglify-js": "2.6.2"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "angular2",
    "karma",
    "jasmine",
    "istanbul",
    "unit-testing",
    "typescript"
  ],
  "bugs": {
    "url": ""
  },
  "homepage": ""
}