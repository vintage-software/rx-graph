{
  "name": "vstack-graph",
  "version": "0.6.3",
  "description": "A RxJS object relation graph for client side JavaScript Applications",
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "start": "npm run test",
    "build": "npm run lint && npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-spec.json && npm run minify-js",
    "pretest": "npm run build",
    "test": "karma start karma.conf.js",
    "posttest": "",
    "coverage": "remap-istanbul -i coverage/coverage-final.json -o coverage -t html && http-server -c-1 -o -p 9875 ./coverage",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "minify-js": "uglifyjs ./bundles/vstack-graph.js -o ./bundles/vstack-graph.min.js",
    "clean": "rm -rf dist && rm -rf bundles",
    "lint": "tslint \"vstack-graph/**/*.ts\""
  },
  "author": "Vintage Software",
  "contributors": [
    {
      "name": "Cory Rylan",
      "email": "crylan@vintagesoftware.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vintage-software/vstack-graph.git"
  },
  "dependencies": {
    "rxjs": "^5.0.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.38",
    "@types/es6-shim": "0.31.32",
    "browserify-istanbul": "^2.0.0",
    "codecov.io": "^0.1.6",
    "core-js": "^2.4.1",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "remap-istanbul": "^0.8.3",
    "tslint": "4.1.1",
    "typescript": "^2.1.4",
    "typings": "^2.1.0",
    "uglify-js": "^2.7.5",
    "watchify": "^3.8.0"
  },
  "keywords": [
    "angular",
    "rxjs",
    "jasmine",
    "typescript",
    "rest",
    "services",
    "falcor"
  ],
  "bugs": {
    "url": "https://github.com/vintage-software/vstack-graph/issues"
  },
  "homepage": "https://github.com/vintage-software/vstack-graph"
}